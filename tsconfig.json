{
  "compilerOptions": {
    "allowJs": false,
    "allowSyntheticDefaultImports": true,
    "experimentalDecorators": true,
    "jsx": "react-native",
    "module": "es2015",
    "moduleResolution": "node",
    "noImplicitAny": false,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "sourceMap": true,
    "target": "esnext",
    "lib": ["esnext", "dom"],
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "baseUrl": "./", /* Base directory to resolve non-absolute module names. */
    "paths": {
      "@components/*": [
        "./app/components/*"
      ],
      "@screens/*": [
        "./app/screens/*"
      ],
      "@assets/*": [
        "./assets/*"
      ],
      "@actions/*": [
        "./app/redux/actions/*"
      ],
      "@store/*": [
        "./app/store/*"
      ],
      "@models/*": [
        "./app/models/*"
      ],
      "@services/*": [
        "./app/services/*"
      ],
      "@theme/*": [
        "./app/theme/*"
      ],
      "@utils/*": [
        "./app/utils/*"
      ],
      "@navigators/*": [
        "./app/navigators/*"
      ],
      "@apis/*": [
        "./app/apis/*"
      ],
      "@images":[
        "./app/assets/images.ts"
      ]
    },
  },
  "exclude": ["node_modules"],
  "include": ["index.js", "app", "test", "storybook"]
}
